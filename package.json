    "ng": "ng",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "start": "ng serve",
    "prod": "ng serve --configuration=production --prod --ssl --ssl-key ssl/server.key --ssl-cert ssl/server.crt --port 4200",
    "dev": "ng serve --configuration=dev --ssl --ssl-key ssl/server.key --ssl-cert ssl/server.crt --port 4200",
    "hmr": "node --max_old_space_size=8086 ./node_modules/@angular/cli/bin/ng serve --configuration=hmr --hmr  --ssl --ssl-key ssl/server.key --ssl-cert ssl/server.crt --port 4200",
    "local": "node --max_old_space_size=8086 ./node_modules/@angular/cli/bin/ng serve --configuration=local --ssl --ssl-key ssl/server.key --ssl-cert ssl/server.crt --port 4200",
    "prod:build": "npm run set-asset-version && node --max_old_space_size=8086 ./node_modules/@angular/cli/bin/ng  build --configuration=production --aot=true --prod --output-hashing=all --build-optimizer=true --source-map=false --named-chunks=false --vendor-chunk=false && npm run trans:optimize",
    "dev:build": "npm run set-asset-version && node --max_old_space_size=8086 ./node_modules/@angular/cli/bin/ng  build --configuration=dev --aot=true --prod --output-hashing=all --build-optimizer=true --source-map=false --named-chunks=false --vendor-chunk=false && npm run trans:optimize",
    "prod:stats": "ng build --configuration=production --prod --stats-json",
    "dev:stats": "ng build --configuration=dev --stats-json",
    "analyze": "webpack-bundle-analyzer dist/CloudApperClient/stats.json --port=4201",
    "trans:optimize": "transloco-optimize dist/CloudApperClient/assets/i18n",
    "trans:find": "transloco-keys-manager find --add-missing-keys -e",
    "trans:extract": "transloco-keys-manager extract -d {{key}}",
    "trans:extract-all": "transloco-keys-manager extract -l en bn fr -d {{key}}",
    "set-asset-version": "node ./versioning.build.js",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
